<form method="post" action="{{ path('app_user_delete', {'id': user|raw.id}) }}" onsubmit="return confirm('{{ 'Are you sure you want to delete this item'|trans|raw }}');" style="display:inline">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user|raw.id) }}">
	<button class="btn btn-link text-decoration-none text-orange p-0">
		{{ 'Btn.Delete'|trans|raw }}
	</button>
</form>
